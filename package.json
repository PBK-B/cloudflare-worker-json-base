{
    "name": "worker-json-base",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "deploy": "npm run build && wrangler deploy",
        "dev": "wrangler dev --port 8788",
        "dev:worker": "wrangler dev --port 8788",
        "dev:webui": "vite",
        "start": "npm run build && npm run dev:worker",
        "build": "tsc",
        "build:worker": "tsc",
        "build:webui": "vite build",
        "build:all": "npm run build:worker && npm run build:webui",
        "type-check": "tsc --noEmit",
        "type-check:worker": "tsc --noEmit",
        "test": "npm run test:worker && npm run test:webui",
        "test:worker": "jest --config jest.worker.config.json",
        "test:worker:watch": "jest --config jest.worker.config.json --watch",
        "test:worker:coverage": "jest --config jest.worker.config.json --coverage",
        "test:webui": "jest --config jest.webui.config.json",
        "test:webui:watch": "jest --config jest.webui.config.json --watch",
        "test:webui:coverage": "jest --config jest.webui.config.json --coverage",
        "webui": "vite",
        "webui:build": "vite build",
        "webui:preview": "vite preview",
        "webui:serve": "vite preview --port 3000",
        "clean": "rm -rf dist dist-webui",
        "d1:init": "npx wrangler d1 create jsonbase --local",
        "d1:migrate": "npx wrangler d1 execute jsonbase --local --file=./src/database/schema.sql",
        "d1:reset": "npx wrangler d1 execute jsonbase --local --command=\"DROP TABLE IF EXISTS data_items; DROP TABLE IF EXISTS schema_migrations;\" && npm run d1:migrate",
        "d1:view": "npx wrangler d1 execute jsonbase --local --command=\"SELECT * FROM data_items ORDER BY updated_at DESC\"",
        "d1:stats": "npx wrangler d1 execute jsonbase --local --command=\"SELECT COUNT(*) as total, SUM(size) as total_size FROM data_items\""
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20260124.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@types/inquirer": "^9.0.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^25.0.10",
        "@vitejs/plugin-react": "^5.1.2",
        "chalk": "^5.0.0",
        "commander": "^12.0.0",
        "identity-obj-proxy": "^3.0.0",
        "inquirer": "^9.0.0",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "less": "^4.5.1",
        "ts-jest": "^29.4.6",
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "wrangler": "^4.60.0"
    },
    "dependencies": {
        "@rsuite/icons": "^1.4.0",
        "@types/react": "^19.2.9",
        "@types/react-dom": "^19.2.3",
        "axios": "^1.13.2",
        "axios-hooks": "^5.1.1",
        "lucide-react": "^0.563.0",
        "mobx": "^6.15.0",
        "mobx-react-lite": "^4.1.1",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-router-dom": "^7.13.0",
        "rsuite": "^5.83.4",
        "sass": "^1.97.3"
    }
}